{
  "name": "frontend",
  "scripts": {
    "clean": "rm -rf build/",
    "finished": "echo -e '\n――― Frontend build finished! ―――\n'",
    "build:dev": "webpack --config webpack.dev.js && npm run --silent finished",
    "build:prod": "webpack --config webpack.prod.js && npm run --silent finished",
    "build:release": "webpack --config webpack.prod.js && npm run --silent finished",
    "build": "func() { npm run --silent clean && npm run build:\"$1\"; }; func",
    "start": "webpack-dev-server --config webpack.dev_server.js --env.dev_server"
  },
  "devDependencies": {
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.19.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "graphql": "^14.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
